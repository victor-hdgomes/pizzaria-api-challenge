generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

enum Role {
  USER
  ASSISTANT
}

model Message {
  id        Int      @id @default(autoincrement())
  role      Role
  content   String
  createdAt DateTime @default(now())
}
